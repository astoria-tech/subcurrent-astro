    "@astrojs/tailwind": "^5.1.0",
    "@extractus/feed-extractor": "^7.1.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "astro": "^5.3.0",
    "dotenv": "^16.4.7",
    "isomorphic-dompurify": "^2.22.0",
    "lucide-react": "^0.479.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2"
  }
